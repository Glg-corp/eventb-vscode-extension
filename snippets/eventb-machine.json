        },
        "event-any-where-then": {
            "prefix": "event-any-where-then",
            "body": "event ${1:event_name}\nany $0\nwhere\n\t\nthen\n\t\nend",
            "description": "Snippet for event-any-where-then block"
        },
        "events": {
            "prefix": "events",
            "body": "events\n\t$0",
            "description": "Snippet for a generic block"
        },
        "label": {
