{
    "source.eventb.machine": {
        "invariants": {
            "prefix": [
                "inv",
                "invariants"
            ],
            "body": "invariants\n\t@${1:label}: $0\nend\n",
            "description": "Snippet for invariants block"
        },
        "event-then": {
            "prefix": [
                "event-then",
                "et"
            ],
            "body": "event ${1:event_name}\nthen\n\t$0\nend\n",
            "description" : "Snippet for event-then block"
        },
        "event-where-then": {
            "prefix" : [
                "event-where-then",
                "ewt"
            ],
            "body" : "event ${1:event_name}\nwhere\n\t${2:@guard: my_guard}\nthen\n\t${3:@action: my_action}\nend\n",
            "description" : "Snippet for event-where-then block"
        },
        "event-any-where-then": {
            "prefix" : [
                "event-any-where-then",
                "eawt"
            ],
            "body" : "event ${1:event_name}\nany ${2:parameter}\nwhere\n\t${3:@guard: my_guard}\nthen\n\t${3:@action: my_action}\nend\n",
            "description" : "Snippet for event-any-where-then block"            
        },
        "block" : {
            "prefix" : [
                "block"
            ],
            "body" : "${1|invariants,events,event|}\n\t$0\nend\n",
            "description" : "Snippet for a generic block"
        },
        "label" : {
            "prefix" : [
                "lbl",
                "label"
            ],
            "body" : "@${1:label}: $0",
            "description" : "Snippet for a label block"
        },
        "variables": {
            "prefix" : [
                "var",
                "variables"
            ],
            "body" : "variables ${0:my_var}",
            "description" : "Snippet for a variables block"
        },
        "machine" : {
            "prefix" : ["mach","machine"],
            "body" : "machine $TM_FILENAME_BASE\n"
        },
        "char_arrows":{
            "prefix" : "char_arrows",
            "body":"${1|⇒,⇔,↦,→,↠,↦,,,}"
        },
        "char_op" : {
            "prefix" : "char_op",
            "body" : "${1|+,−,^,∗,÷,×|}"
        },

        "char_logical":{
            "prefix": "char_logical",
            "body":"${1|∧,∨,¬,∀,∃|}"
        },
        "char_set_oper" : {
            "prefix" : [
                "char_set_oper"
            ],
            "body" : "${1|∈,⊂,⊆,∀,∃,∪,∩,⋂,⋃,⊄,⊈,∉,:∈|}"
        },
        "char_set" : {
            "prefix" : "char_set",
            "body" : "${1|ℕ,ℕ1,ℙ,ℙ1,ℤ,∅|}"
        },
        "char_common" : {
            "prefix" : "char_common",
            "body" : "${1|≔,≠,≤,≥,¬,∈,∧,∨|}"
        }
    }
}