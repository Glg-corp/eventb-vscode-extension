{
    "source.eventb.machine": {
        "invariants": {
            "prefix": "invariants",
            "body": "invariants\n\t@${1:label}: $0",
            "description": "Snippet for invariants block"
        },
        "event-then": {
            "prefix": "event-then",
            "body": "event ${1:event_name}\nthen\n\t$0\nend",
            "description": "Snippet for event-then block"
        },
        "event-where-then": {
            "prefix": "event-where-then",
            "body": "event ${1:event_name}\nwhere\n\t$0\nthen\n\t\nend",
